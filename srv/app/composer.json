{
    "name": "shippeo/php-ms-template",
    "description": "",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "~8.3.0",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-igbinary": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo_pgsql": "*",
        "ext-redis": "*",
        "ext-sockets": "*",
        "ext-tokenizer": "*",
        "beberlei/assert": "^3.3",
        "doctrine/annotations": "^1.0",
        "doctrine/dbal": "^3.3",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/migrations": "^3.5",
        "monolog/monolog": "^2.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.5",
        "psr/cache": "^3.0",
        "psr/log": "^3.0",
        "shippeo/metrics-bundle": "~0.5.0",
        "symfony/amqp-messenger": "~6.4.0",
        "symfony/asset": "~6.4.0",
        "symfony/browser-kit": "~6.4.0",
        "symfony/cache": "~6.4.0",
        "symfony/cache-contracts": "^3.1",
        "symfony/config": "~6.4.0",
        "symfony/console": "~6.4.0",
        "symfony/debug-bundle": "~6.4.0",
        "symfony/dependency-injection": "~6.4.0",
        "symfony/doctrine-messenger": "~6.4.0",
        "symfony/dotenv": "~6.4.0",
        "symfony/event-dispatcher": "~6.4.0",
        "symfony/finder": "~6.4.0",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "~6.4.0",
        "symfony/http-foundation": "~6.4.0",
        "symfony/http-kernel": "~6.4.0",
        "symfony/lock": "~6.4.0",
        "symfony/messenger": "~6.4.0",
        "symfony/monolog-bundle": "^3.7",
        "symfony/property-access": "~6.4.0",
        "symfony/property-info": "~6.4.0",
        "symfony/routing": "~6.4.0",
        "symfony/runtime": "~6.4.0",
        "symfony/security-bundle": "~6.4.0",
        "symfony/security-core": "~6.4.0",
        "symfony/security-http": "~6.4.0",
        "symfony/serializer": "~6.4.0",
        "symfony/service-contracts": "^3.1",
        "symfony/uid": "~6.4.0",
        "symfony/validator": "~6.4.0",
        "symfony/yaml": "~6.4.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.23",
        "fakerphp/faker": "^1.19",
        "friends-of-phpspec/phpspec-expect": "^4.0",
        "helmich/phpunit-json-assert": "^3.4",
        "phpspec/phpspec": "^7.2",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-beberlei-assert": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-symfony": "^1.1",
        "phpunit/phpunit": "^10.5",
        "symfony/css-selector": "~6.4.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/var-dumper": "~6.4.0",
        "symfony/web-profiler-bundle": "~6.4.0",
        "zenstruck/messenger-test": "^1.4"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "repositories": {
        "shippeo": {
            "type": "composer",
            "url": "https://php-packages.shippeo.cloud",
            "only": [
                "shippeo/*"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\PHPUnit\\": "tests/phpunit",
            "App\\Tests\\Shared\\": "tests/shared",
            "spec\\App\\": "tests/spec/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "docker": false,
            "endpoint": [
                "https://api.github.com/repos/shippeo/symfony-flex-recipes/contents/index.json",
                "flex://defaults"
            ],
            "require": "6.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "composer normalize"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
